{
  "name": "secure-kit",
  "version": "0.1.1",
  "description": "Production-grade security + performance toolkit for backend frameworks with OWASP Top 10 compliance",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "secure-backend": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./express": {
      "import": "./dist/adapters/express.js",
      "require": "./dist/adapters/express.js",
      "types": "./dist/adapters/express.d.ts"
    },
    "./koa": {
      "import": "./dist/adapters/koa.js",
      "require": "./dist/adapters/koa.js",
      "types": "./dist/adapters/koa.d.ts"
    },
    "./fastify": {
      "import": "./dist/adapters/fastify.js",
      "require": "./dist/adapters/fastify.js",
      "types": "./dist/adapters/fastify.d.ts"
    },
    "./nest": {
      "import": "./dist/adapters/nest.js",
      "require": "./dist/adapters/nest.js",
      "types": "./dist/adapters/nest.d.ts"
    }
  },
  "files": [
    "dist",
    "scripts",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "docs"
  ],
  "scripts": {
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest --testPathPattern=security",
    "test:performance": "jest --testPathPattern=performance",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"examples/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"examples/**/*.ts\" \"tests/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "docs": "typedoc src/index.ts --out docs --exclude '**/*.test.ts' --exclude '**/cli/**' --exclude '**/scripts/**'",
    "docs:serve": "npm run docs && npx http-server docs -p 8080",
    "docs:build": "npm run docs",
    "docs:deploy": "npm run docs && gh-pages -d docs",
    "security-audit": "npm audit --audit-level=high",
    "prepublishOnly": "npm run build:clean && npm run test:ci && npm run lint && npm run type-check",
    "postinstall": "node scripts/postinstall.js",
    "prepare": "husky install",
    "release": "npm run prepublishOnly && npm publish",
    "release:dry": "npm run prepublishOnly && npm publish --dry-run",
    "security:audit": "npx secure-backend audit",
    "security:check": "npm audit && npx secure-backend audit",
    "dev:secure": "NODE_ENV=development node examples/server.ts"
  },
  "keywords": [
    "security",
    "middleware",
    "express",
    "koa",
    "fastify",
    "nestjs",
    "helmet",
    "cors",
    "csrf",
    "rate-limiting",
    "authentication",
    "performance",
    "owasp",
    "typescript",
    "backend",
    "api-security",
    "http-security",
    "compression",
    "caching",
    "jwt",
    "file-upload-security",
    "input-sanitization",
    "dos-protection",
    "graphql-security"
  ],
  "author": "Secure Backend Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaDevZone/secure-kit.git"
  },
  "bugs": {
    "url": "https://github.com/MetaDevZone/secure-kit/issues"
  },
  "homepage": "https://metadevzone.github.io/secure-kit/",
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/koa": "^2.15.0",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^6.21.0",
    "cookie-parser": "^1.4.7",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^1.7.1",
    "express": "^4.18.2",
    "fastify": "^4.24.3",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "koa": "^2.14.2",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.1.0",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "express": ">=4.0.0",
    "fastify": ">=4.0.0",
    "koa": ">=2.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}